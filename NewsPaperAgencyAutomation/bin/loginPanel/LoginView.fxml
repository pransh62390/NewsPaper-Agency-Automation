<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane minHeight="200.0" minWidth="350.0" style="-fx-background-color: skyblue;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16" fx:controller="loginPanel.LoginViewController">
   <children>
      <Label alignment="CENTER" prefHeight="35.0" prefWidth="350.0" style="-fx-background-color: black;" text="LoginPage" textFill="WHITE">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <PasswordField fx:id="pswd" layoutX="66.0" layoutY="87.0" prefHeight="30.0" prefWidth="200.0" promptText="Enter password here" />
      <Text layoutX="66.0" layoutY="75.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password" wrappingWidth="200.0">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Text>
      <Button layoutX="122.0" layoutY="128.0" mnemonicParsing="false" onAction="#checkPswd" prefHeight="30.0" prefWidth="100.0" text="Login">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
